{
  "name": "webbuilder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install": "npm-run-all -p install-client install-server",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "start": "npm-run-all -p build-client start-client start-server",
    "build-client": "cd client && npm run build",
    "start-client": "cd client && npm run serve",
    "start-server": "cd server && npm start",
    "test": "npm-run-all -s test-server test-client",
    "test-client": "cd client && npm test",
    "test-server": "cd server && npm test",
    "coverage": "nyc npm test"
  },
  "dependencies": {
    "npm-run-all": "4.1.5"
  },
  "devDependencies": {
    "nyc": "14.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VictorWinberg/webbuilder.git"
  },
  "author": "VictorWinberg, AnnieLeonia",
  "license": "ISC"
}
